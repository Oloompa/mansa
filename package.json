{
  "name": "kanedama",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MansaGroup/kanedama.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MansaGroup/kanedama/issues"
  },
  "homepage": "https://github.com/MansaGroup/kanedama#readme",
  "devDependencies": {
    "@fimbul/wotan": "^0.24.0",
    "@types/jest": "28.1.3",
    "@types/node": "^18.0.0",
    "c8": "^7.11.3",
    "prettier": "2.7.1",
    "pta": "^1.1.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "zora": "^5.0.4"
  },
  "scripts": {
    "pre-install": "git config core.hooksPath .githooks",
    "clean:dist": "find . -path '**/node_modules/*' -prune -o -name 'dist' -type d -prune -print -exec rm -rfI '{}' + ",
    "clean:build": "find . -path '**/node_modules/*' -prune -o -name 'build' -type d -prune -print -exec rm -rfI '{}' + ",
    "clean:deps": "find . \\( -name 'node_modules' -type d -o -name 'package-lock.json' -type f \\) -prune -print -exec rm -rfI '{}' + "
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  },
  "workspaces": [
    "backend",
    "frontend"
  ]
}
